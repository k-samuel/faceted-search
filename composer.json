{
    "name": "k-samuel/faceted-search",
    "version": "3.2.2",
    "type": "library",
    "description": "PHP Faceted search",
    "keywords": ["php","faceted search"],
    "homepage": "https://github.com/k-samuel/faceted-search",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/k-samuel/faceted-search/issues",
        "source": "https://github.com/k-samuel/faceted-search"
    },
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "php":">=7.4.0"
    },
    "autoload": {
      "psr-4": {
        "KSamuel\\FacetedSearch\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "KSamuel\\FacetedSearch\\Tests\\Benchmark\\": "tests/benchmark"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "phpstan/phpstan": "^2",
        "codacy/coverage": "dev-master",
        "phpbench/phpbench": "^1.2"
    },
    "archive": {
        "exclude": ["/examples"]
    }
}
